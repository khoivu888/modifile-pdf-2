{
  "manifest_version": 3,
  "name": "Iframe Div Click Listener",
  "version": "1.0",
  "permissions": ["activeTab", "downloads"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "all_frames": true
    }
  ]
}
